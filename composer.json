    "keywords": [
        "symfony",
        "doctrine",
        "grumphp",
        "task",
        "validation",
        "scheme",
        "commit"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jonmldr",
            "email": "jonmldr@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpro/grumphp": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JonMldr\\GrumPhpDoctrineTask\\": "src/"
        }
    }
}
